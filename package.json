{
  "name": "jupyterlab_tangram",
  "version": "0.1.0",
  "description": "A Jupyterlab extension to render geo-data with Tangram",
  "author": "hanbyul jo <hanbyul@descarteslabs.com>",
  "main": "lib/lab_extension.js",
  "files": [
    "lib/"
  ],
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "scripts": {
    "build": "webpack --config webpack.config.js"
  },
