    "mimeExtension": "lib/lab_extension"
  },
  "dependencies": {
    "@phosphor/widgets": "^1.5.0",
    "js-yaml": "^3.11.0",
    "leaflet": "^1.3.1",
    "tangram": "^0.15.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.18.2",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-class-properties": "^6.19.0",
    "babel-preset-latest": "^6.16.0",
    "css-loader": "^0.25.0",
    "file-loader": "^0.9.0",
    "json-loader": "^0.5.4",
    "mkdirp": "^0.5.1",
    "npm-run-all": "^4.1.1",
    "rimraf": "^2.6.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.0"
  }
}
